Cell[TextData[{
 "The body fixed x unit vector, ",
 Cell[BoxData[
  FormBox[
   SubscriptBox[
    RowBox[{"{", 
     SubscriptBox[
      OverscriptBox["x", "\[RightVector]"], 
      OverscriptBox["B", "_"]], "}"}], 
    OverscriptBox["G", "_"]], TraditionalForm]],ExpressionUUID->
  "dd1805f6-160f-445e-8f9f-d80d44971b2b"],
 " is then calculated by rotating ",
 Cell[BoxData[
  FormBox[
   SubscriptBox[
    RowBox[{"{", 
     SubscriptBox[
      OverscriptBox["y", "\[RightVector]"], 
      OverscriptBox["T", "_"]], "}"}], 
    OverscriptBox["G", "_"]], TraditionalForm]],ExpressionUUID->
  "397f057b-fdf0-4f64-b215-b3674928a76c"],
