Cell[BoxData[
 RowBox[{
  RowBox[{"matlabRenameRules", " ", "=", " ", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"\[Phi]", "->", "phi"}], ",", 
     RowBox[{"\[Theta]", "\[Rule]", "theta"}], ",", 
     RowBox[{"\[Psi]", "\[Rule]", "psi"}], ",", 
     RowBox[{"\[Omega]", "\[Rule]", "omega"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      SubscriptBox["u", "rilc"], "\[Rule]", "urilc"}], ",", 
     RowBox[{
      SubscriptBox["u", "rfb"], "\[Rule]", "urfb"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      SubscriptBox["u", "wilc"], "\[Rule]", "uwilc"}], ",", 
     RowBox[{
      SubscriptBox["v", "sp"], "\[Rule]", "vsp"}], ",", 
     RowBox[{
      SubscriptBox["k", "pw"], "\[Rule]", "kpw"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      SubscriptBox["\[Phi]", "0"], "\[Rule]", "phi0"}], ",", 
     RowBox[{
      SubscriptBox["\[Phi]", "1"], "\[Rule]", "phi1"}], ",", 
     RowBox[{
      SubscriptBox["\[Theta]", "0"], "\[Rule]", "theta0"}], ",", 
     RowBox[{
      SubscriptBox["\[Theta]", "1"], "\[Rule]", "theta1"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{"\[Sigma]", "\[Rule]", "sigma"}], ",", 
     RowBox[{"\[Rho]", "\[Rule]", "rho"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      SubscriptBox["C", "L0"], "\[Rule]", "CL0"}], ",", 
     RowBox[{
      SubscriptBox["C", "L1"], "\[Rule]", "CL1"}], ",", 
     RowBox[{
      SubscriptBox["C", "D0"], "\[Rule]", "CD0"}], ",", 
     RowBox[{
      SubscriptBox["C", "D1"], "\[Rule]", "CD1"}], ",", 
     RowBox[{
      SubscriptBox["C", "D2"], "\[Rule]", "CD2"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      SubscriptBox["v", "app"], "\[Rule]", "vapp"}], ",", 
     RowBox[{
      SubscriptBox["A", "ref"], "\[Rule]", "Aref"}], ",", 
     RowBox[{"\[Tau]", "\[Rule]", "tau"}]}], "}"}]}], ";"}]], "Input",
