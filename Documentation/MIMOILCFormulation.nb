 Cell[BoxData[
  FormBox[
   SubscriptBox[
    RowBox[{"{", 
     SubscriptBox[
      OverscriptBox["u", "\[RightVector]"], "r"], "}"}], 
    OverscriptBox["G", "_"]], TraditionalForm]],ExpressionUUID->
  "550430a6-4b6f-4cf1-b0f1-8cb30a5ea5bf"],
